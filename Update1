GachaRoll.TextSize = 14.000
GachaRoll.TextWrapped = true
GachaRoll.MouseButton1Down:connect(function()
	while true do wait()
		local args = {
			[1] = "Gacha",
			[2] = "Pay Cash",
		}

		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args)) 
	end
end)

