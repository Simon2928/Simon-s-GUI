-- Scripts:

local function QLJL_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ScrollingFrame.Visible = not script.Parent.Parent.ScrollingFrame.Visible
	end)
end
coroutine.wrap(QLJL_fake_script)()
local function UOSDDG_fake_script() -- ProjectStar.LocalScript 
	local script = Instance.new('LocalScript', ProjectStar)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.ProjectStar.Visible = not script.Parent.Parent.Parent.ProjectStar.Visible
	end)
end
coroutine.wrap(UOSDDG_fake_script)()
local function GWTJ_fake_script() -- YBA.LocalScript 
	local script = Instance.new('LocalScript', YBA)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.YBA.Visible = not script.Parent.Parent.Parent.YBA.Visible
	end)
end
coroutine.wrap(GWTJ_fake_script)()
local function MXQMTBY_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Frame.Visible = not script.Parent.Parent.Frame.Visible
	end)
end
coroutine.wrap(MXQMTBY_fake_script)()
